<?xml version="1.0" encoding="UTF-8" ?>
<flashtool-config version="2.0">
    <general>
        <chip-name>MT6737M</chip-name>
        <storage-type>EMMC</storage-type>
        <download-agent>boxdata\da\MTK_AllInOne_DA.bin</download-agent>
        <scatter>boxdata\mtk\1\MT6737M_Android_EMMC_scatter.txt</scatter>
        <authentication></authentication>
        <certification></certification>
        <rom-list>
            <rom index="0" enable="false"></rom>
            <rom index="6" enable="false"></rom>
            <rom index="8" enable="false"></rom>
            <rom index="9" enable="false"></rom>
            <rom index="10" enable="false"></rom>
            <rom index="14" enable="false"></rom>
            <rom index="16" enable="false"></rom>
            <rom index="17" enable="false"></rom>
            <rom index="20" enable="false"></rom>
