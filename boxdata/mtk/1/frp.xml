            <rom index="23" enable="false"></rom>
            <rom index="24" enable="false"></rom>
            <rom index="25" enable="false"></rom>
            <rom index="26" enable="false"></rom>
            <rom index="27" enable="false"></rom>
            <rom index="28" enable="false"></rom>
            <rom index="29" enable="false"></rom>
            <rom index="30" enable="false"></rom>
