        <storage-type>EMMC</storage-type>
        <download-agent>boxdata\da\MTK_AllInOne_DA_N.bin</download-agent>
        <scatter>boxdata\mtk\1\MT6761_Android_scatter.txt</scatter>
        <authentication></authentication>
        <certification></certification>
        <rom-list>
            <rom index="0" enable="false"></rom>
            <rom index="3" enable="false"></rom>
            <rom index="16" enable="false"></rom>
            <rom index="17" enable="false"></rom>
            <rom index="18" enable="false"></rom>
            <rom index="19" enable="false"></rom>
            <rom index="20" enable="false"></rom>
