        <log-info log_on="true" log_path="C:\ProgramData\SP_FT_Logs" clean_hours="744" />
    </general>
    <commands>
        <combo-format>
		<format validation="false" physical="false" erase-flag="NormalErase" auto-format="false">
		<begin-addr>0x3588000</begin-addr>
		<length>0x100000</length>
		<part-id>3</part-id>
		</format>
		</combo-format>
    </commands>
</flashtool-config>
